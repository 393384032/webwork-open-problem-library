Context("Numeric");
Context()->variables->add(L=>"Real");
Context()->variables->add(M=>"Real");
Context()->variables->add(T=>"Real");

#computation 
$a = "L T**(-2)";
$omega = "T**(-1)";
$alpha = "T**(-2)";

#set tolerance
Context()->flags->set(
tolerance=>.01);

