Context("Numeric");
#given
$load = 1000;
$l = 2;
$lt = 5;
$do = 2;
$di = 1.5;

#image scale
$imgScale = .2;

#image aspect ratio
$width = $imgScale*2489;
$height = $imgScale*706;

#computation 
$ra = $l/$lt*$load;
$rc = ($l - $lt)/$lt*$load;


