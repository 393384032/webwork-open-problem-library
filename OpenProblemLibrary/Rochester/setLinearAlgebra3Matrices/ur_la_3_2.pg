DOCUMENT() ;        

loadMacros(
"PGstandard.pl",
"MathObjects.pl",
"parserVectorUtils.pl",
"parserPopUp.pl",
"PGcourse.pl"
) ;

$showPartialCorrectAnswers = 1 ;

############

TEXT(beginproblem());

Context('Matrix');

do {

$A = Matrix([
[non_zero_random(-4,4,1),random(-4,4,1),random(-4,4,1)],
[random(-4,4,1),random(-4,4,1),non_zero_random(-4,4,1)],
[random(-4,4,1),non_zero_random(-4,4,1),random(-4,4,1)]
]);

$B = Matrix([
[non_zero_random(-4,4,1),random(-4,4,1),random(-4,4,1)],
[random(-4,4,1),random(-4,4,1),non_zero_random(-4,4,1)],
[random(-4,4,1),non_zero_random(-4,4,1),random(-4,4,1)]
]);

$AB = $A * $B;
$BA = $B * $A;

} until ($AB != $BA);

$popup = PopUp(["Choose","True","False"],"False");

Context()->texStrings;
BEGIN_TEXT
If \(A = $A\) and \(B = $B\), then
$BR
$BR
\(AB=\) \{ $AB->ans_array() \} and
$BR
$BR
\( BA=\) \{ $BA->ans_array() \}.
$BR
$BR
\{ $popup->menu() \} True or False: \( AB = BA \) for any two square matrices \( A \) and \( B \) of the same size.
END_TEXT
Context()->normalStrings;


ANS( $AB->cmp() );
ANS( $BA->cmp() );
ANS( $popup->cmp() );

COMMENT('MathObject version');

ENDDOCUMENT() ;        







