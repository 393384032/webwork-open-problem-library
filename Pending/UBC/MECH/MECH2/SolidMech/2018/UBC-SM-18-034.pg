Context("Numeric");
#given
$l = 1;
$f = 500;

#image scale
$imgScale = .2;

#image aspect ratio
$width = $imgScale*1816;
$height = $imgScale*1725;

#computation 
$fs = $f/4;
$ds = $fs * $l / ($l * $l * 5);

